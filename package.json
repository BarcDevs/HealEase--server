{
    "name": "express-ts-api",
    "version": "0.0.0",
    "description": "",
    "lint-staged": {
        "src/**/*.ts": [
            "prettier --write",
            "eslint"
        ],
        "*.js": "eslint --cache --fix"
    },
    "main": "index.js",
    "scripts": {
        "start": "NODE_ENV=default nodemon src/app.ts",
        "start:dev": "NODE_ENV=development nodemon src/app.ts",
        "start:prod": "NODE_ENV=production node build/index.bundle.js",
        "prod": "npm run prisma:generate && npm run prisma:deploy && webpack",
        "test": "NODE_ENV=test jest --watchAll --detectOpenHandles --forceExit --coverage",
        "format": "prettier --write 09",
        "lint:check": "eslint 09",
        "lint:fix": "npm run lint:check -- --fix",
        "prepare": "husky install",
        "prisma:generate": "npx prisma generate",
        "prisma:migrate": "npx prisma migrate dev --name init",
        "prisma:deploy": "npx prisma migrate deploy",
        "prisma:studio": "npx prisma studio"
    },
